# Staging Environment Variables
NODE_ENV=production
NEXT_PUBLIC_API_URL=https://staging.example.com/api

# Application Settings
NEXT_TELEMETRY_DISABLED=1

# Performance
ENABLE_CACHE=true
CACHE_TTL=3600

# Monitoring
ENABLE_MONITORING=true
SENTRY_DSN=https://your-sentry-dsn@sentry.io/staging
SENTRY_ENVIRONMENT=staging
SENTRY_TRACES_SAMPLE_RATE=0.1

# Logging
LOG_LEVEL=debug
ENABLE_REQUEST_LOGGING=true

# Security
ENABLE_RATE_LIMITING=true
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Feature Flags
ENABLE_BETA_FEATURES=true
ENABLE_A_B_TESTING=true

# External Services
ANALYTICS_ID=UA-STAGING-ID

# Database (if applicable)
DATABASE_URL=postgresql://user:pass@staging-db.example.com:5432/filename_generator_staging

# Redis (if applicable)
REDIS_URL=redis://staging-redis.example.com:6379
# Auto-updated user permissions

# Auto-updated user permissions
USER_ID=1005
GROUP_ID=1003
