# Staging Environment Variables
NODE_ENV=production
ENVIRONMENT=staging
NEXT_PUBLIC_API_URL=http://localhost:18090/api

# Network Configuration
SUBNET_PREFIX=10.102.0
GATEWAY_IP=10.102.0.1
NGINX_IP=10.102.0.10
APP_IP=10.102.0.20
REDIS_IP=10.102.0.30

# Port Configuration
NGINX_PORT=18090
NGINX_SECURE_PORT=18090
APP_INTERNAL_PORT=3000
REDIS_INTERNAL_PORT=6379

# Network Names
NETWORK_NAME=filename-generator-staging-network
BRIDGE_NAME=staging-bridge

# Container Names
NGINX_CONTAINER=filename-generator-nginx-staging
APP_CONTAINER=filename-generator-staging
REDIS_CONTAINER=filename-generator-redis-staging

# Hostnames
NGINX_HOSTNAME=nginx-staging.local
APP_HOSTNAME=app-staging.local
REDIS_HOSTNAME=redis-staging.local

# Application Settings
NEXT_TELEMETRY_DISABLED=1

# Performance
ENABLE_CACHE=true
CACHE_TTL=3600

# Monitoring
ENABLE_MONITORING=true
SENTRY_DSN=https://your-sentry-dsn@sentry.io/staging
SENTRY_ENVIRONMENT=staging
SENTRY_TRACES_SAMPLE_RATE=0.1

# Logging
LOG_LEVEL=debug
ENABLE_REQUEST_LOGGING=true

# Security
ENABLE_RATE_LIMITING=true
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Feature Flags
ENABLE_BETA_FEATURES=true
ENABLE_A_B_TESTING=true

# External Services
ANALYTICS_ID=UA-STAGING-ID

# Database (if applicable)
DATABASE_URL=postgresql://user:pass@staging-db.example.com:5432/filename_generator_staging

# Redis (if applicable)
REDIS_URL=redis://staging-redis.example.com:6379