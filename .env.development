# Development Environment Variables
NODE_ENV=development
ENVIRONMENT=development
NEXT_PUBLIC_API_URL=http://localhost:18090/api

# Network Configuration
SUBNET_PREFIX=10.100.0
GATEWAY_IP=10.100.0.1
NGINX_IP=10.100.0.10
APP_IP=10.100.0.20
REDIS_IP=10.100.0.30

# Port Configuration
NGINX_PORT=18090
APP_INTERNAL_PORT=3000
REDIS_INTERNAL_PORT=6379

# Network Names
NETWORK_NAME=filename-generator-dev-network
BRIDGE_NAME=dev-bridge

# Container Names
NGINX_CONTAINER=filename-generator-nginx-dev
APP_CONTAINER=filename-generator-app-dev
REDIS_CONTAINER=filename-generator-redis-dev

# Hostnames
NGINX_HOSTNAME=nginx-dev.local
APP_HOSTNAME=app-dev.local
REDIS_HOSTNAME=redis-dev.local

# Development Features
NEXT_TELEMETRY_DISABLED=1
ANALYZE=false

# Development Server
PORT=3000
HOSTNAME=localhost

# Logging
LOG_LEVEL=debug
ENABLE_DEBUG_LOGS=true

# Hot Reload
WATCHPACK_POLLING=true

# Development Tools
ENABLE_SOURCE_MAPS=true
DISABLE_ESLINT_PLUGIN=false

# Mock Services (for development)
USE_MOCK_API=false
MOCK_API_DELAY=500