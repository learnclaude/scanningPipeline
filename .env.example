# Copy this file to .env.local for local development
NEXT_PUBLIC_API_URL=http://localhost:3000/api

# Network Configuration - External Interface IP for Docker containers
EXTERNAL_INTERFACE_IP=192.168.1.100

# Production ports
NGINX_EXTERNAL_PORT=80
NGINX_EXTERNAL_HTTPS_PORT=443

# Development port
NGINX_DEV_PORT=8090

# Test port
NGINX_TEST_PORT=18092

# Staging ports
NGINX_STAGING_PORT=18093
NGINX_STAGING_HTTPS_PORT=18094

# Environment
NODE_ENV=production

# User permissions for development

# User permissions (will be auto-updated by scripts)

# User permissions (will be auto-updated by scripts)
USER_ID=1000
GROUP_ID=1000
