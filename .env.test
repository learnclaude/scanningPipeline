# Test Environment Variables
NODE_ENV=test
ENVIRONMENT=test
NEXT_PUBLIC_API_URL=http://localhost:18090/api

# Network Configuration
SUBNET_PREFIX=10.101.0
GATEWAY_IP=10.101.0.1
NGINX_IP=10.101.0.10
APP_IP=10.101.0.20

# Port Configuration
NGINX_PORT=18090
APP_INTERNAL_PORT=3000

# Network Names
NETWORK_NAME=filename-generator-test-network
BRIDGE_NAME=test-bridge

# Container Names
NGINX_CONTAINER=filename-generator-nginx-test
APP_CONTAINER=filename-generator-test-app

# Hostnames
NGINX_HOSTNAME=nginx-test.local
APP_HOSTNAME=app-test.local

# User Synchronization
USER_ID=1000
GROUP_ID=1000

# Git Configuration
GIT_REPO_URL=https://github.com/learnclaude/sivaDemo.git
GIT_BRANCH=testing

# Test Configuration
CI=true
SKIP_ENV_VALIDATION=true

# Test Database (if applicable)
TEST_DATABASE_URL=postgresql://test:test@localhost:5432/filename_generator_test

# Test Features
ENABLE_TEST_FEATURES=true
MOCK_EXTERNAL_SERVICES=true

# Test Timeouts
TEST_TIMEOUT=30000
E2E_TEST_TIMEOUT=60000

# Coverage
COLLECT_COVERAGE=true
COVERAGE_THRESHOLD_GLOBAL=80

# Test Output
JEST_JUNIT_OUTPUT_DIR=./test-results
JEST_JUNIT_OUTPUT_NAME=junit.xml